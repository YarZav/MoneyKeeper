platform :ios, '11.0'

def working_pods
use_frameworks!
    pod 'SnapKit', :inhibit_warnings => true
    pod 'YZNumPad', :git => 'https://github.com/YarZav/YZNumPad.git', :tag => '1.0.1'
    pod 'YZBarChart', :git => 'https://github.com/YarZav/YZBarChart.git', :branch => 'master'
    pod 'YZNotificationView', :git => 'https://github.com/YarZav/YZNotificationView.git', :tag => '1.0.2'
    pod 'YZPinCodeViewController', :git => 'https://github.com/YarZav/YZPinCodeViewController.git', :branch => 'develop'
    pod 'EANBarcodeGenerator'
end

target 'MoneyKeeper' do
    working_pods
end

